{
	"format": "page",
	"id": "case",
	"label": "Case",
	"repeatable": true,
	"status": true,
	"json": {
		"Main": {
			"uid": {
				"type": "UID",
				"config": {
					"label": "UID"
				}
			},
			"title": {
				"type": "Text",
				"config": {
					"label": "Title",
					"placeholder": "Title of portfolio item"
				}
			},
			"portfolio_image": {
				"type": "Image",
				"config": {
					"label": "Portfolio Image",
					"constraint": {},
					"thumbnails": []
				}
			},
			"intro": {
				"type": "StructuredText",
				"config": {
					"label": "intro",
					"placeholder": "Explain the case briefly",
					"allowTargetBlank": false,
					"multi": "paragraph,strong,em"
				}
			},
			"slices": {
				"type": "Slices",
				"fieldset": "Slice Zone",
				"config": {
					"choices": {
						"quote": {
							"type": "SharedSlice"
						},
						"text_with_image": {
							"type": "SharedSlice"
						},
						"image": {
							"type": "SharedSlice"
						},
						"text": {
							"type": "SharedSlice"
						}
					}
				}
			}
		},
		"SEO & Metadata": {
			"meta_title": {
				"config": {
					"label": "Meta Title",
					"placeholder": "A title of the page used for social media and search engines"
				},
				"type": "Text"
			},
			"meta_description": {
				"config": {
					"label": "Meta Description",
					"placeholder": "A brief summary of the page"
				},
				"type": "Text"
			},
			"meta_image": {
				"config": {
					"constraint": {
						"height": 1260,
						"width": 2400
					},
					"label": "Meta Image",
					"thumbnails": []
				},
				"type": "Image"
			}
		}
	}
}
